{
  "extends": "airbnb",
  "globals": {
    "wp": true
  },
  "env": {
    "amd": true,
    "browser": true,
    "jquery": true,
    "node": true
  },
  "rules": {
    "no-var": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "comma-dangle": 0,
    "prefer-template": 0,
    "object-shorthand": 0,
  },
  "parser": "babel-eslint"
}
